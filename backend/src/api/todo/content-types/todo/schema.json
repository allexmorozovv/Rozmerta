{
  "kind": "collectionType",
  "collectionName": "todos",
  "info": {
    "singularName": "todo",
    "pluralName": "todos",
    "displayName": "todo"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "date": {
      "type": "date"
    },
    "priority": {
      "type": "enumeration",
      "enum": [
        "minor",
        "regular",
        "critical"
      ],
      "default": "regular",
      "required": true
    },
    "done": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    }
  }
}
